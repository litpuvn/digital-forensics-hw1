<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#ffd48e8e" android:endColor="#ff291b1b" android:angle="270.0" />
            <stroke android:width="1.0dip" android:color="#ff7f5555" />
            <corners android:radius="10.0dip" />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape>
            <gradient android:startColor="#ffba8686" android:endColor="#ff0f0b0b" android:angle="270.0" />
            <stroke android:width="1.0dip" android:color="#ff664949" />
            <corners android:radius="10.0dip" />
        </shape>
    </item>
</selector>