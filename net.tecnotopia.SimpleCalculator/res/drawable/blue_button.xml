<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#ff0066ff" android:endColor="#ff002254" android:angle="270.0" />
            <stroke android:width="1.0dip" android:color="#ff0043a8" />
            <corners android:radius="10.0dip" />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape>
            <gradient android:startColor="#ff267dff" android:endColor="#ff0d2954" android:angle="270.0" />
            <stroke android:width="1.0dip" android:color="#ff1952a8" />
            <corners android:radius="10.0dip" />
        </shape>
    </item>
</selector>