<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#ff00d46a" android:endColor="#ff002914" android:angle="270.0" />
            <stroke android:width="1.0dip" android:color="#ff007f40" />
            <corners android:radius="10.0dip" />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape>
            <gradient android:startColor="#ff00ba5d" android:endColor="#ff000f08" android:angle="270.0" />
            <stroke android:width="1.0dip" android:color="#ff006633" />
            <corners android:radius="10.0dip" />
        </shape>
    </item>
</selector>